  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "clean": "npm run clean:app && rm -fr public/js/__build__",
    "clean:app": "rm -fr out",
    "build:schema": "node out/scripts/updateSchema.js",
    "build:langs": "TRANSLATE=yes node out/scripts/translate.js && mkdir -p out/build/lang && cp -r build/lang out/build/ && cp -r build/babelRelayPlugin.js out/build/",
    "build:app": "npm run clean:app && mkdir -p out/data/database && mkdir -p out/scripts && mkdir -p out/build && cp build/rewrite-modules.js out/build/ && node_modules/.bin/babel --presets es2015,stage-0,react scripts/updateSchema.js --out-dir out/ && node_modules/.bin/babel --presets es2015,stage-0,react scripts/translate.js --out-dir out/ && cp data/database/*.json out/data/database/ && node_modules/.bin/babel --presets es2015,stage-0,react data/ --out-dir out/data/ && node_modules/.bin/babel --presets es2015,stage-0,react server.js --out-dir out/ && node_modules/.bin/babel --presets es2015,stage-0,react webpack.config.js --out-dir out/",
    "build": "npm run build:app && npm run build:schema && npm run build:langs",
    "start": "node out/server.js",
    "build:langs:dev": "node -r dotenv/config out/scripts/translate.js && mkdir -p out/build/lang && cp -r build/lang out/build/ && cp -r build/babelRelayPlugin.js out/build/",
    "build:dev": "npm run build:app && npm run build:schema && npm run build:langs:dev",
    "start:dev": "node -r dotenv/config out/server.js"
  },
